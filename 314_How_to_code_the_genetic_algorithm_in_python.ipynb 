        "n_bits = len(numerical_feature_names)\n",
        "\n",
        "# Define genetic algorithm parameters\n",
        "n_iter = 100\n",
        "n_pop = 100\n",
        "r_cross = 0.9\n",
        "r_mut = 1.0 / float(n_bits)\n",
        "\n",
